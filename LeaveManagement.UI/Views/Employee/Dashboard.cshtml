<h3 class="m-2">DashBoard</h3>
<!-- Image and text -->
<nav class="navbar navbar-light bg-dark">
    <a class="navbar-brand" href="#">
        <img width="40" height="35" class="d-inline-block align-top" alt="" src=" data:image /png;base64,">
        <span class="fw-bold text-white">Welcome,@ViewBag.MySession</span>
    </a>
</nav>

<div class="row">
    <div class="col-sm-3 mt-5">
        <div class="card">
            <div class="card-body bg-secondary">
                <h5 class="card-title text-white">Apply For Leave</h5>
                <a asp-controller="Leave" asp-action="EmployeeLeaves" class="btn btn-primary">Apply</a>
            </div>
        </div>
    </div>
    <div class="col-sm-3 mt-5">
        <div class="card">
            <div class="card-body bg-secondary">
                <h5 class="card-title text-white">Add New Employee</h5>
                <a asp-controller="Employee" asp-action="Create" class="btn btn-primary">Add</a>
            </div>
        </div>
    </div>
</div>

<a asp-controller="Employee" asp-action="Logout" class="btn btn-danger mt-5">Logout</a>
